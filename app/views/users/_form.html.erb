<%= form_with(model: user, local: true) do |f| %>  
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  
  <%= f.label :prof_image %>
  <%= f.file_field :prof_image %>
  
  <%= f.label :prof_content %>
  <%= f.text_field :prof_content %>
  
  <%= f.submit "Create my account" %>
  <% end %>